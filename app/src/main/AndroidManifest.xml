<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cyd.learnandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LearnAndroid">
        <activity android:name=".customview.SurfaceViewActivity"></activity>
        <activity android:name=".ui.TestLeakActivity" />
        <activity android:name=".customview.PostFabActivity" />
        <activity
            android:name=".customview.MaskActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".customview.EulerActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".customview.CanvasActivity" />
        <activity android:name=".ui.FragmentActivity" />
        <activity android:name=".ui.IntentActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="cyd"
                    android:scheme="example" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>